{
  "source": "tushare",
  "id": "monthly",
  "api_name": "monthly",
  "enabled": true,
  "group": "market",
  "frequency": "monthly",
  "description": "股票月线行情数据",
  "params": {
    "start_date": "{last_month}",
    "end_date": "{trade_date}"
  },
  "batch_size": 3000,
  "index_fields": ["ts_code", "trade_date"],
  "fields_to_store": [
    "amount", "low", "close", "pre_close", "high", 
    "pct_chg", "vol", "trade_date", "ts_code", "open", "change"
  ],
  "wan_mode": 0
} 