{
    "source": "tushare",
    "id": "ggt_monthly",
    "api_name": "ggt_monthly",
    "enabled": true,
    "group": "market",
    "frequency": "monthly",
    "description": "港股通每月成交统计",
    "params": {
      "start_date": "{last_month}",
      "end_date": "{trade_date}"
    },
    "batch_size": 10000,
    "index_fields": ["month","markettype"],
    "fields_to_store": [
        "month", "markettype", "day_buy_amt", "day_buy_vol", "day_sell_amt", 
        "day_sell_vol", "total_buy_amt", "total_buy_vol", "total_sell_amt", "total_sell_vol"
    ],
    "wan_mode": 0
  } 